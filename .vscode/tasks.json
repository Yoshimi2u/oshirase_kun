{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Flutter Build Release (iOS)",
			"type": "shell",
			"command": "flutter",
			"args": [
				"build",
				"ios",
				"--release",
				"--no-codesign"
			],
			"group": "build",
			"isBackground": false,
			"problemMatcher": [
				"$dart-flutter"
			]
		}
	]
}